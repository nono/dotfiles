set bell-style none

set meta-flag on
set input-meta on
set convert-meta off
set output-meta on
set show-all-if-ambiguous on

set mark-symlinked-directories off

"\e[1~": beginning-of-line
"\e[4~": end-of-line
"\e[5~": history-search-backward
"\e[6~": history-search-forward
"\e[3~": delete-char
"\e[2~": quoted-insert
"\e[5C": forward-word
"\e[5D": backward-word
"\eOH": beginning-of-line
"\eOF": end-of-line

